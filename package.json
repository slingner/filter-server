    "jsonwebtoken": "^8.5.1",
    "knex": "^0.20.8",
    "morgan": "^1.9.1",
    "pg": "^7.18.1",
    "treeize": "^2.1.2",
    "winston": "^3.2.1",
    "xss": "^1.0.6"
  },
  "devDependencies": {
